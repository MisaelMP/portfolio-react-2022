{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "dist"
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/index.html"
		}
	],
	"github": {
		"enabled": true,
		"silent": false,
		"deployments": {
			"production": ["master"],
			"preview": ["development"]
		}
	}
}
